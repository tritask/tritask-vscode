{
    "name": "tritask",
    "displayName": "Tritask syntax",
    "description": "Tritask syntax definition for .trita files.",
    "publisher": "stakiran",
    "repository": "https://github.com/tritask/tritask-vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tritask",
                "aliases": [
                    "Tritask",
                    "trita",
                    "Trita"
                ],
                "extensions": [
                    ".trita"
                ]
            }
        ],
        "grammars": [
            {
                "language": "tritask",
                "scopeName": "text.trita",
                "path": "./syntaxes/trita.tmLanguage.json"
            }
        ]
    }
}
